  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      },
      "cookieAuthUserId": {
        "type": "apiKey",
        "in": "cookie",
        "name": "sessionId"
      },
      "cookieAuthRefreshToken": {
        "type": "apiKey",
        "in": "cookie",
        "name": "refreshToken"
      }
    },
    "schemas": {
      "contact": {
        "type": "object",
        "required": [
          "name",
          "phoneNumber",
          "contactType",
          "userId"
        ],
        "properties": {
          "_id": {
            "type": "string",
            "description": "Contact id",
            "example": "65ca67e7ae7f10c88b598384"
          },
          "name": {
            "type": "string",
            "description": "Contact name",
            "example": "Mariia Pavlenko"
          },
          "phoneNumber": {
            "type": "string",
            "description": "Contact phone number",
            "example": "+380671338855"
          },
          "email": {
            "type": "string",
